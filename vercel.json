{
    "version": 2,
    "builds": [
        {
            "src": "src/api/requirements.txt",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/api/index.py"
        }
    ],
    "python": {
        "version": "3.10"
    }
}
