{
    "version": 2,
    "builds": [
        {
            "src": "src/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/server.js"
        }
    ],
    "builds": [
        {
            "src": "install-python-deps.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "public"
            }
        }
    ]
}
